{
    "title":"@ui.param.title",
    "popup":true,
    "fullscreen":true,
    "fixed":true,
    "temp":true,
    "static":true,
    "nobar":true,
    "transparent":true,
    "ui.property.style":{
        "left": "5%",
        "top": "@ui.param.top(5%)",
        "height": "@ui.param.height(5%)",
        "width": "90%",
        "pointerEvents":"none"
    },
    "ui.basic.elements": [
        {
            "ui.basic.tag":"div",
            "ui.class.class":"widget.transform.layout",
            "ui.class.classExtra":"modal.playing.layout",
            "ui.basic.var":"layout",
            "ui.basic.html":"@ui.param.layout",
            "ui.style.fontSize":"@ui.param.fontSize"
        }
    ]
}
