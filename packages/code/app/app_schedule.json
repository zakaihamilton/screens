{
    "title": "Schedule",
    "ui.property.style": {
        "left": "50px",
        "top": "50px",
        "width": "600px",
        "height": "600px"
    },
    "maximize": true,
    "icon": "/packages/res/icons/schedule.png",
    "widget.menu.items": [
        {
            "text": "Schedule",
            "select": [
                {
                    "text": "Refresh",
                    "select": "app.schedule.reset"
                },
                {
                    "text": "View",
                    "options": {
                        "separator": true
                    },
                    "select": [
                        {
                            "text": "Year",
                            "select": "app.schedule.viewType",
                            "options": {
                                "state": "select"
                            }
                        },
                        {
                            "text": "Month",
                            "select": "app.schedule.viewType",
                            "options": {
                                "state": "select"
                            }
                        },
                        {
                            "text": "Week",
                            "select": "app.schedule.viewType",
                            "options": {
                                "state": "select"
                            }
                        },
                        {
                            "text": "Day",
                            "select": "app.schedule.viewType",
                            "options": {
                                "state": "select"
                            }
                        }
                    ]
                },
                {
                    "text": "First Day of Week",
                    "select": [
                        {
                            "text": "Saturday",
                            "select": "app.schedule.firstDay",
                            "options": {
                                "state": "select"
                            }
                        },
                        {
                            "text": "Sunday",
                            "select": "app.schedule.firstDay",
                            "options": {
                                "state": "select"
                            }
                        },
                        {
                            "text": "Monday",
                            "select": "app.schedule.firstDay",
                            "options": {
                                "state": "select"
                            }
                        }
                    ]
                },
                {
                    "text": "Group",
                    "select": [
                        {
                            "text": "All",
                            "select": "app.schedule.group",
                            "options": {
                                "state": "select",
                                "value": ""
                            }
                        },
                        "app.schedule.groupMenuList"
                    ]
                }
            ]
        }
    ],
    "ui.basic.elements": [
        {
            "ui.element.component": "widget.schedule",
            "methods": {
                "event": "app.schedule.event",
                "previous": "app.schedule.previous",
                "next": "app.schedule.next",
                "today": "app.schedule.today"
            },
            "ui.basic.var": "schedule"
        },
        {
            "ui.element.component": "widget.spinner",
            "ui.basic.var": "spinner",
            "ui.style.visibility": "hidden"
        }
    ],
    "core.link.work": "app.schedule.work",
    "ui.property.after": {
        "ui.resize.event": "app.schedule.refresh",
        "storage.local.key": "app-schedule-window",
        "storage.local.restore": "widget.window.store"
    }
}