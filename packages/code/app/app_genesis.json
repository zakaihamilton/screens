{
    "title": "Genesis",
    "ui.property.style": {
        "left": "50px",
        "top": "50px",
        "width": "1200px",
        "height": "600px"
    },
    "ui.resize.event": "app.genesis.update",
    "ui.basic.elements": [
        {
            "ui.class.class": "app.genesis.canvas",
            "mode": "2d",
            "ui.basic.var":"canvas",
            "ui.style.backgroundColor": "white"
        }
    ],
    "ui.property.after": {
        "storage.local.key": "app-genesis-window",
        "storage.local.restore": "widget.window.store",
        "app.genesis.update":null
    }
}