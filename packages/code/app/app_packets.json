{
    "title": "Packets",
    "icon": "packets",
    "ui.property.style": {
        "left": "450px",
        "top": "200px",
        "width": "600px",
        "height": "450px"
    },
    "widget.menu.items": [
        {
            "text": "Packets",
            "select": [
                {
                    "text": "Refresh",
                    "select": "app.packets.refreshData"
                },
                {
                    "text": "Auto Refresh",
                    "select": "app.packets.autoRefresh",
                    "options": {
                        "state": "select"
                    }
                },
                {
                    "text": "Reset",
                    "select": "app.packets.reset",
                    "options": {
                        "separator": true
                    }
                },
                {
                    "text": "Push Packets",
                    "select": "app.packets.pushPackets",
                    "options": {
                        "state": "select",
                        "separator": true
                    }
                },
                {
                    "text": "Collect Packets",
                    "select": "app.packets.collectPackets",
                    "options": {
                        "state": "select"
                    }
                },
                {
                    "text": "Combine Packets",
                    "select": "app.packets.combinePackets",
                    "options": {
                        "state": "select"
                    }
                },
                {
                    "text": "Filters",
                    "select": "header"
                },
                {
                    "text": "Monitor Filter",
                    "select": "label"
                },
                {
                    "text": "Enter filter here...",
                    "options": {
                        "edit": "app.packets.monitorFilter"
                    }
                },
                {
                    "text": "Search Filter",
                    "select": "label"
                },
                {
                    "text": "Enter filter here...",
                    "options": {
                        "edit": "app.packets.searchFilter"
                    }
                }
            ]
        },
        {
            "text": "Data",
            "select": [
                {
                    "text": "Title",
                    "options": {
                        "edit": "app.packets.dataTitle"
                    }
                },
                {
                    "text": "Refresh",
                    "select": "app.packets.refreshDataList",
                    "options": {
                        "separator": true
                    }
                },
                {
                    "text": "Live",
                    "select": "app.packets.dataProfile",
                    "options": {
                        "state": "select",
                        "separator": true
                    }
                },
                {
                    "text": "Combined",
                    "select": "app.packets.dataProfile",
                    "options": {
                        "state": "select",
                        "separator": true
                    }
                },
                "app.packets.dataMenuList",
                {
                    "text": "Save",
                    "select": "app.packets.save",
                    "options": {
                        "enabled": "select",
                        "separator": true
                    }
                }
            ]
        },
        {
            "text": "View",
            "select": [
                {
                    "text": "Auto",
                    "select": "app.packets.viewType",
                    "options": {
                        "state": "select"
                    }
                },
                {
                    "text": "Data by Time",
                    "select": "app.packets.viewType",
                    "options": {
                        "state": "select",
                        "separator": true
                    }
                },
                {
                    "text": "ABR by Packet Delay",
                    "select": "app.packets.viewType",
                    "options": {
                        "state": "select"
                    }
                },
                {
                    "text": "Duration by Packet Delay",
                    "select": "app.packets.viewType",
                    "options": {
                        "state": "select"
                    }
                },
                {
                    "text": "Duration % by Packet Delay",
                    "select": "app.packets.viewType",
                    "options": {
                        "state": "select"
                    }
                },
                {
                    "text": "Average Duration by Packet Delay",
                    "select": "app.packets.viewType",
                    "options": {
                        "state": "select"
                    }
                },
                {
                    "text": "Average Duration % by Packet Delay",
                    "select": "app.packets.viewType",
                    "options": {
                        "state": "select"
                    }
                },
                {
                    "text": "Export",
                    "select": "app.packets.export",
                    "options": {
                        "separator": true
                    }
                }
            ]
        },
        {
            "text": "Stream",
            "select": [
                "app.packets.streamMenuList"
            ]
        },
        {
            "text": "Effects",
            "select": [
                {
                    "text": "Refresh",
                    "select": "app.packets.applyEffects"
                },
                {
                    "text": "Enable",
                    "select": "app.packets.useEffects",
                    "options": {
                        "state": "select"
                    }
                },
                {
                    "text": "Bandwidth Rate (KBit)",
                    "select": "label",
                    "options": {
                        "separator": true
                    }
                },
                {
                    "options": {
                        "edit": "app.packets.bandwidthRate"
                    }
                },
                {
                    "text": "Bandwidth Burst (KBit)",
                    "select": "label"
                },
                {
                    "options": {
                        "edit": "app.packets.bandwidthBurst"
                    }
                },
                {
                    "text": "Bandwidth Latency (ms)",
                    "select": "label"
                },
                {
                    "options": {
                        "edit": "app.packets.bandwidthLatency"
                    }
                },
                {
                    "text": "Packet Loss (%)",
                    "select": "label",
                    "options": {
                        "separator": true
                    }
                },
                {
                    "options": {
                        "edit": "app.packets.packetLoss"
                    }
                },
                {
                    "text": "Packet Delay (msec)",
                    "select": "label"
                },
                {
                    "options": {
                        "edit": "app.packets.packetDelay"
                    }
                },
                {
                    "text": "Toggle Interval (sec)",
                    "select": "label",
                    "options": {
                        "separator": true
                    }
                },
                {
                    "options": {
                        "edit": "app.packets.toggleInterval"
                    }
                }
            ],
            "options": {
                "var": "effectsMenu"
            }
        }
    ],
    "ui.basic.elements": [
        {
            "ui.basic.tag": "div",
            "ui.property.style": {
                "display": "flex",
                "margin": "10px",
                "flexWrap": "wrap",
                "alignItems": "flex-start"
            }
        },
        {
            "ui.element.component": "widget.chart",
            "ui.basic.var": "chart",
            "ui.style.paddingLeft": "5%",
            "ui.style.width": "95%",
            "data": "@app.packets.chartData",
            "options": "@app.packets.chartOptions",
            "type": "@app.packets.chartType",
            "info": {}
        }
    ],
    "ui.property.after": {
        "storage.local.key": "app-packets-window",
        "storage.local.restore": "widget.window.store",
        "app.packets.refreshChart": null,
        "app.packets.refreshData": null,
        "app.packets.onChangeStream": null
    }
}