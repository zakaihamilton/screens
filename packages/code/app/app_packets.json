{
    "title": "Packets",
    "icon": "/packages/res/icons/packets.png",
    "ui.property.style": {
        "left": "450px",
        "top": "200px",
        "width": "600px",
        "height": "450px"
    },
    "widget.menu.items": [
        [
            "Packets",
            [
                [
                    "Refresh",
                    "app.packets.refreshData"
                ],
                [
                    "Auto Refresh",
                    "app.packets.autoRefresh",
                    {
                        "state": "select"
                    }
                ],
                [
                    "Reset",
                    "app.packets.reset",
                    {
                        "separator": true
                    }
                ],
                [
                    "Auto Increase Packet Delay",
                    "app.packets.autoIncreasePacketDelay",
                    {
                        "state": "select",
                        "separator": true
                    }
                ],
                [
                    "Push Packets",
                    "app.packets.pushPackets",
                    {
                        "state": "select",
                        "separator": true
                    }
                ],
                [
                    "Collect Packets",
                    "app.packets.collectPackets",
                    {
                        "state": "select"
                    }
                ],
                [
                    "Combine Packets",
                    "app.packets.combinePackets",
                    {
                        "state": "select"
                    }
                ]
            ]
        ],
        [
            "Data",
            [
                [
                    "Refresh",
                    "app.packets.refreshDataList"
                ],
                [
                    "Live",
                    "app.packets.dataProfile",
                    {
                        "state": "select",
                        "separator": true
                    }
                ],
                [
                    "Combined",
                    "app.packets.dataProfile",
                    {
                        "state": "select",
                        "separator": true
                    }
                ],
                "app.packets.dataMenuList",
                [
                    "Save",
                    "app.packets.save",
                    {
                        "enabled": "select",
                        "separator": true
                    }
                ]
            ]
        ],
        [
            "View",
            [
                [
                    "Auto",
                    "app.packets.viewType",
                    {
                        "state": "select"
                    }
                ],
                [
                    "Data by Time",
                    "app.packets.viewType",
                    {
                        "state": "select",
                        "separator": true
                    }
                ],
                [
                    "ABR by Packet Delay",
                    "app.packets.viewType",
                    {
                        "state": "select"
                    }
                ],
                [
                    "Duration by Packet Delay",
                    "app.packets.viewType",
                    {
                        "state": "select"
                    }
                ],
                [
                    "Duration % by Packet Delay",
                    "app.packets.viewType",
                    {
                        "state": "select"
                    }
                ],
                [
                    "Average Duration by Packet Delay",
                    "app.packets.viewType",
                    {
                        "state": "select"
                    }
                ],
                [
                    "Average Duration % by Packet Delay",
                    "app.packets.viewType",
                    {
                        "state": "select"
                    }
                ],
                [
                    "Export",
                    "app.packets.export",
                    {
                        "separator": true
                    }
                ]
            ]
        ],
        [
            "Stream",
            [
                "app.packets.streamMenuList"
            ]
        ]
    ],
    "ui.basic.elements": [
        {
            "ui.basic.tag": "div",
            "ui.property.style": {
                "display": "flex",
                "margin": "10px",
                "flexWrap": "wrap",
                "alignItems": "flex-start"
            },
            "ui.basic.elements": [
                {
                    "ui.basic.tag": "div",
                    "ui.basic.elements": [
                        {
                            "ui.basic.text": "Title",
                            "ui.property.style": {
                                "paddingTop": "10px"
                            }
                        },
                        {
                            "ui.basic.text": "",
                            "ui.property.style": {
                                "width": "200px"
                            },
                            "ui.basic.type": "text",
                            "ui.basic.var": "title"
                        }
                    ]
                },
                {
                    "ui.basic.tag": "div",
                    "ui.basic.elements": [
                        {
                            "ui.basic.text": "Monitor Filter",
                            "ui.property.style": {
                                "paddingTop": "10px"
                            }
                        },
                        {
                            "ui.basic.text": "",
                            "ui.property.style": {
                                "width": "200px"
                            },
                            "ui.basic.type": "text",
                            "ui.basic.var": "monitorFilter",
                            "ui.monitor.change": "app.packets.updateMonitorOptions"
                        }
                    ]
                },
                {
                    "ui.basic.tag": "div",
                    "ui.basic.elements": [
                        {
                            "ui.basic.text": "Search Filter",
                            "ui.property.style": {
                                "paddingTop": "10px"
                            }
                        },
                        {
                            "ui.basic.text": "",
                            "ui.property.style": {
                                "width": "200px"
                            },
                            "ui.basic.type": "text",
                            "ui.basic.var": "searchFilter",
                            "ui.monitor.change": "app.packets.updateMonitorOptions"
                        }
                    ]
                },
                {
                    "ui.basic.tag": "div",
                    "ui.basic.elements": [
                        {
                            "ui.basic.text": "Search Match",
                            "ui.property.style": {
                                "paddingTop": "10px"
                            }
                        },
                        {
                            "ui.basic.text": "",
                            "ui.basic.readOnly": true,
                            "ui.property.style": {
                                "width": "200px"
                            },
                            "ui.basic.type": "text",
                            "ui.basic.var": "searchMatch"
                        }
                    ]
                },
                {
                    "ui.basic.tag": "div",
                    "ui.basic.elements": [
                        {
                            "ui.basic.text": "Bandwidth Rate (KBit)",
                            "ui.property.style": {
                                "paddingTop": "10px"
                            }
                        },
                        {
                            "ui.basic.text": "0",
                            "ui.property.style": {
                                "width": "200px"
                            },
                            "ui.basic.type": "text",
                            "ui.basic.var": "bandwidthRate",
                            "ui.monitor.change": "app.packets.effects"
                        }
                    ]
                },
                {
                    "ui.basic.tag": "div",
                    "ui.basic.elements": [
                        {
                            "ui.basic.text": "Bandwidth Burst (KBit)",
                            "ui.property.style": {
                                "paddingTop": "10px"
                            }
                        },
                        {
                            "ui.basic.text": "0",
                            "ui.property.style": {
                                "width": "200px"
                            },
                            "ui.basic.type": "text",
                            "ui.basic.var": "bandwidthBurst",
                            "ui.monitor.change": "app.packets.effects"
                        }
                    ]
                },
                {
                    "ui.basic.tag": "div",
                    "ui.basic.elements": [
                        {
                            "ui.basic.text": "Bandwidth Latency (ms)",
                            "ui.property.style": {
                                "paddingTop": "10px"
                            }
                        },
                        {
                            "ui.basic.text": "0",
                            "ui.property.style": {
                                "width": "200px"
                            },
                            "ui.basic.type": "text",
                            "ui.basic.var": "bandwidthLatency",
                            "ui.monitor.change": "app.packets.effects"
                        }
                    ]
                },
                {
                    "ui.basic.tag": "div",
                    "ui.basic.elements": [
                        {
                            "ui.basic.text": "Packet Loss (%)",
                            "ui.property.style": {
                                "paddingTop": "10px"
                            }
                        },
                        {
                            "ui.basic.text": "",
                            "ui.property.style": {
                                "width": "200px"
                            },
                            "ui.basic.type": "number",
                            "ui.basic.var": "packetLoss",
                            "ui.attribute.step": "0.0001",
                            "ui.monitor.change": "app.packets.effects",
                            "ui.attribute.list": "packetLoss"
                        },
                        {
                            "ui.basic.tag": "datalist",
                            "ui.basic.elementId": "packetLoss",
                            "ui.group.data": {
                                "ui.data.keyList": [
                                    "ui.attribute.value"
                                ],
                                "ui.data.default": {
                                    "ui.basic.tag": "option"
                                },
                                "ui.data.values": [
                                    [
                                        "0.0001"
                                    ],
                                    [
                                        "0.001"
                                    ],
                                    [
                                        "0.1"
                                    ],
                                    [
                                        "1"
                                    ]
                                ]
                            }
                        }
                    ]
                },
                {
                    "ui.basic.tag": "div",
                    "ui.basic.elements": [
                        {
                            "ui.basic.text": "Packet Delay (msec)",
                            "ui.property.style": {
                                "paddingTop": "10px"
                            }
                        },
                        {
                            "ui.basic.text": "0",
                            "ui.property.style": {
                                "width": "200px"
                            },
                            "ui.attribute.min": "0",
                            "ui.attribute.step": "5",
                            "ui.attribute.max": "1000",
                            "ui.basic.type": "number",
                            "ui.basic.var": "packetDelay",
                            "ui.monitor.change": "app.packets.effects"
                        }
                    ]
                }
            ]
        },
        {
            "ui.element.component": "widget.chart",
            "ui.basic.var": "chart",
            "ui.style.paddingLeft": "10%",
            "ui.style.width": "75%",
            "data": "@app.packets.chartData",
            "options": "@app.packets.chartOptions",
            "type": "@app.packets.chartType",
            "info": {}
        }
    ],
    "ui.property.after": {
        "storage.local.key": "app-packets-window",
        "storage.local.restore": "widget.window.store",
        "app.packets.refreshChart": null,
        "app.packets.refreshData": null,
        "app.packets.onChangeStream": null
    },
    "app.packets.loadMonitorOptions": null
}