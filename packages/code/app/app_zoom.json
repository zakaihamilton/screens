{
    "title": "Zoom",
    "ui.property.style": {
        "left": "50px",
        "top": "50px",
        "width": "300px",
    "height": "220px"
    },
    "icon": "/packages/res/icons/zoom.png",
    "ui.basic.elements": [
        {
            "ui.property.style":{
                "position":"absolute",
                "left":"10px",
                "top":"30px"
            },
            "ui.basic.text":"Join Url: "
        },
        {
            "ui.basic.tag":"a",
            "ui.property.style":{
                "position":"absolute",
                "left":"30px",
                "top":"60px"
            },
            "ui.basic.href":"@app.zoom.joinUrl",
            "ui.basic.text":"@app.zoom.joinUrl"
        },
        {
            "ui.property.style":{
                "position":"absolute",
                "left":"10px",
                "top":"90px"
            },
            "ui.basic.text":"Meeting ID: "
        },
        {
            "ui.property.style":{
                "position":"absolute",
                "left":"10px",
                "top":"115px"
            },
            "ui.basic.readOnly": true,
            "ui.element.component": "widget.input",
            "ui.basic.text":"@app.zoom.meetingId"
        }
    ],
    "ui.property.after":{
        "storage.local.key": "app-zoom-window",
        "storage.local.restore": "widget.window.store"
    }
}
