{
    "title": "Zoom",
    "ui.property.style": {
        "left": "150px",
        "top": "150px",
        "width": "500px",
        "height": "120px"
    },
    "icon": "/packages/res/icons/zoom.png",
    "ui.basic.elements": [
        {
            "ui.property.style":{
                "position":"absolute",
                "left":"10px",
                "top":"30px"
            },
            "ui.basic.text":"Join Url: "
        },
        {
            "ui.basic.tag":"a",
            "ui.property.style":{
                "position":"absolute",
                "right":"20px",
                "top":"30px"
            },
            "ui.basic.href":"@app.zoom.joinUrl",
            "ui.basic.text":"@app.zoom.joinUrl"
        },
        {
            "ui.property.style":{
                "position":"absolute",
                "left":"10px",
                "top":"60px"
            },
            "ui.basic.text":"Meeting ID: "
        },
        {
            "ui.property.style":{
                "position":"absolute",
                "right":"20px",
                "top":"55px"
            },
            "ui.basic.readOnly": true,
            "ui.element.component": "widget.input",
            "ui.basic.text":"@app.zoom.meetingId"
        }
    ],
    "ui.property.after":{
        "storage.local.key": "app-version-window",
        "storage.local.restore": "widget.window.store"
    }
}
