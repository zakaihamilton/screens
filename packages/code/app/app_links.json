{
    "title": "Links",
    "ui.property.style": {
        "left": "50px",
        "top": "50px",
        "width": "600px",
        "height": "600px"
    },
    "maximize": null,
    "icon": "links.png",
    "widget.menu.items": [
        {
            "text": "Links",
            "select": [
                {
                    "text": "Clear",
                    "select": "app.links.clear"
                },
                {
                    "text": "Edit Mode",
                    "select": "app.links.editMode",
                    "options": {
                        "state": "select",
                        "separator": true
                    }
                }
            ]
        },
        {
            "text": "Layout",
            "select": [
                {
                    "text": "Border",
                    "select": "app.links.border",
                    "options": {
                        "state": "select"
                    }
                },
                {
                    "text": "Font Size",
                    "select": "header"
                },
                "widget.transform.fontSizesPx(app.links.fontSize)"
            ]
        },
        {
            "text": "Content",
            "select": [
                "app.links.content.menu"
            ]
        }
    ],
    "ui.basic.elements": [
        {
            "ui.basic.tag": "div",
            "ui.basic.var": "links",
            "ui.class.class": "app.links.widget"
        }
    ],
    "ui.property.after": {
        "storage.local.key": "app-links-window",
        "storage.local.restore": "widget.window.store",
        "title": "Links",
        "name": "",
        "app.links.initOptions": null,
        "app.links.reload": null
    }
}