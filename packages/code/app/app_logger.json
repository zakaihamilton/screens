{
    "title": "Logger",
    "ui.property.style": {
        "left": "120px",
        "top": "120px",
        "width": "640px",
        "height": "480px"
    },
    "icon": "/packages/res/icons/logger.png",
    "widget.menu.items": [
        {
            "text": "Logger",
            "select": [
                {
                    "text": "Refresh",
                    "select": "app.logger.refresh"
                },
                {
                    "text": "Clear",
                    "select": "app.logger.clear"
                },
                {
                    "text": "Enabled",
                    "select": "app.logger.enable",
                    "options": {
                        "state": "select",
                        "separator": true
                    }
                },
                {
                    "text": "Server",
                    "select": "app.logger.source",
                    "options": {
                        "state": "select",
                        "separator": true
                    }
                },
                {
                    "text": "Client",
                    "select": "app.logger.source",
                    "options": {
                        "state": "select"
                    }
                },
                {
                    "text": "Browser",
                    "select": "app.logger.source",
                    "options": {
                        "state": "select"
                    }
                },
                {
                    "text": "Service",
                    "select": "app.logger.source",
                    "options": {
                        "state": "select"
                    }
                },
                {
                    "text": "Service Worker",
                    "select": "app.logger.source",
                    "options": {
                        "state": "select"
                    }
                }
            ]
        },
        {
            "text": "View",
            "select": [
                {
                    "text": "All",
                    "select": "app.logger.type",
                    "options": {
                        "value": "core.console.retrieveMessages",
                        "state": "select"
                    }
                },
                {
                    "text": "Errors",
                    "select": "app.logger.type",
                    "options": {
                        "value": "core.console.retrieveErrors",
                        "state": "select"
                    }
                },
                {
                    "text": "Filter",
                    "select": "label"
                },
                {
                    "text": "Enter filter here...",
                    "options": {
                        "edit": "app.logger.filter"
                    }
                }
            ]
        }
    ],
    "ui.basic.html": "@app.logger.html",
    "ui.property.after": {
        "storage.local.key": "app-logger-window",
        "storage.local.restore": "widget.window.store",
        "app.logger.refresh": null
    }
}