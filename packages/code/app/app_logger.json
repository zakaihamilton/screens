{
    "title": "Logger",
    "ui.property.style": {
        "left": "120px",
        "top": "120px",
        "width": "640px",
        "height": "480px"
    },
    "icon": "/packages/res/icons/logger.png",
    "widget.menu.items": [
        [
            "Logger",
            [
                [
                    "Refresh",
                    "app.logger.refresh"
                ],
                [
                    "Clear",
                    "app.logger.clear"
                ],
                [
                    "Enabled",
                    "app.logger.enable",
                    {
                        "state": "select",
                        "separator": true
                    }
                ],
                [
                    "Server",
                    "app.logger.source",
                    {
                        "state": "select",
                        "separator": true
                    }
                ],
                [
                    "Client",
                    "app.logger.source",
                    {
                        "state": "select"
                    }
                ],
                [
                    "Browser",
                    "app.logger.source",
                    {
                        "state": "select"
                    }
                ],
                [
                    "Service",
                    "app.logger.source",
                    {
                        "state": "select"
                    }
                ],
                [
                    "Service Worker",
                    "app.logger.source",
                    {
                        "state": "select"
                    }
                ]
            ]
        ],
        [
            "Type",
            [
                [
                    "All",
                    "app.logger.type",
                    {
                        "value":"core.console.retrieveMessages",
                        "state": "select"
                    }
                ],
                [
                    "Errors",
                    "app.logger.type",
                    {
                        "value":"core.console.retrieveErrors",
                        "state": "select"
                    }
                ]
            ]
        ]
    ],
    "ui.class.load":"app.logger",
    "ui.basic.html": "@app.logger.html",
    "ui.property.after": {
        "storage.local.key": "app-logger-window",
        "storage.local.restore": "widget.window.store",
        "app.logger.refresh": null
    }
}