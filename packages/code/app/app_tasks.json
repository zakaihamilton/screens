{
    "title": "Task List",
    "fixed": true,
    "popup": true,
    "temp": true,
    "ui.property.style":{
        "width": "288px",
        "height": "201px"
    },
    "ui.basic.elements": [
        {
            "ui.basic.var": "tasks",
            "ui.element.count": 8,
            "ui.style.marginLeft": "4px",
            "ui.style.marginTop": "4px",
            "ui.style.marginRight": "3px",
            "ui.style.marginBottom": "9px",
            "ui.style.height":"91px",
            "ui.group.data": {
                "ui.data.default": {
                    "group":"tasks",
                    "ui.touch.default":"app.tasks.switchTo"
                },
                "ui.data.keyList": ["ui.basic.text", "state"],
                "ui.data.values": [
                    "app.tasks.tasks"
                ]
            }
        }
    ],
    "ui.group.data": {
        "ui.data.default": {
            "ui.style.marginLeft": "2px",
            "ui.style.marginRight": "10px",
            "ui.style.marginTop": "9px",
            "ui.style.float": "left",
            "ui.style.width": "73px"
        },
        "ui.data.keyList": ["ui.basic.text", "ui.touch.click", "ui.style.width", "ui.style.marginRight"],
        "ui.data.values": [
            ["Switch to", "app.tasks.switchTo"],
            ["End Task", "app.tasks.closeTask"],
            ["Cancel", "widget.window.close", "90px", "0px"],
            ["Cascade", "app.tasks.cascade"],
            ["Tile", "app.tasks.tile"],
            ["Arrange Icons", "ui.arrange.arrangeIcons", "90px", "0px"]
        ]
    }
}
