{
    "title": "Diagram",
    "ui.property.style": {
        "left": "50px",
        "top": "50px",
        "width": "1200px",
        "height": "600px"
    },
    "icon": "/packages/res/icons/diagram.png",
    "widget.menu.items": [
        {
            "text": "Transform",
            "select": [
                {
                    "text": "Refresh",
                    "select": "app.diagram.reload",
                    "options": {
                        "separator": true
                    }
                },
                "app.diagram.transform.menu"
            ]
        },
        {
            "text": "Layout",
            "select": [
                {
                    "text": "Font Size",
                    "select": "header"
                },
                "widget.transform.fontSizesPx(app.diagram.fontSize)"
            ]
        }
    ],
    "ui.basic.elements": [
        {
            "ui.basic.tag": "div",
            "ui.basic.var": "viewer"
        },
        "app.diagram.transform.popup"
    ],
    "ui.property.after": {
        "storage.local.key": "app-diagram-window",
        "storage.local.restore": "widget.window.store",
        "app.diagram.initOptions": null,
        "app.diagram.reload": null
    }
}