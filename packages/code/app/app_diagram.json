{
    "title": "Diagram",
    "ui.property.style": {
        "left": "50px",
        "top": "50px",
        "width": "1200px",
        "height": "600px"
    },
    "icon": "/packages/res/icons/diagram.png",
    "widget.menu.items": [
        [
            "Transform",
            [
                [
                    "Refresh",
                    "app.diagram.reload",
                    {
                        "separator": true
                    }
                ],
                "app.diagram.transform.menu"
            ]
        ],
        [
            "Layout",
            [
                [
                    "Font Size",
                    "header"
                ],
                "widget.transform.fontSizesPx(app.diagram.fontSize)"
            ]
        ]
    ],
    "ui.basic.elements": [
        {
            "ui.basic.tag": "div",
            "ui.basic.var": "viewer"
        },
        {
            "ui.basic.var": "popup",
            "ui.basic.tag": "div",
            "ui.basic.html": "@widget.transform.html",
            "ui.class.add": "modal",
            "ui.style.zIndex": "1000"
        }
    ],
    "ui.property.after": {
        "storage.local.key": "app-diagram-window",
        "storage.local.restore": "widget.window.store",
        "app.diagram.initOptions": null,
        "app.diagram.reload": null
    }
}