{
    "title": "Profile",
    "icon": "/packages/res/icons/profile.png",
    "ui.property.style": {
        "left": "10%",
        "top": "10%",
        "width": "80%",
        "height": "80%"
    },
    "ui.class.load":"app.profile",
    "ui.basic.html": "@app.profile.html",
    "ui.property.after": {
        "storage.local.key": "app-profile-window",
        "storage.local.restore": "widget.window.store"
    },
    "widget.menu.items": [
        [
            "Profile",
            [
                [
                    "This Device",
                    "app.profile.userName",
                    {
                        "separator":true,
                        "state": "select",
                        "value":""
                    }
                ],
                [
                    "Users",
                    null,
                    {
                        "enabled": false,
                        "header": true
                    }
                ],
                "app.profile.userMenuList"
            ],
            {
                "visible":"@user.verify.match(none,Zakai Hamilton)"
            }
        ]
    ],
    "app.profile.initOptions": null,
    "app.profile.updateUser":null,
    "widget.window.maximize":null
}
