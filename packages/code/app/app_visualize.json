{
    "title": "Visualize",
    "ui.property.style": {
        "left": "50px",
        "top": "50px",
        "width": "600px",
        "height": "600px"
    },
    "icon": "/packages/res/icons/diagram.png",
    "widget.menu.items": [
        {
            "text": "Transform",
            "select": [
                "app.visualize.transform.menu"
            ]
        }
    ],
    "storage.local.key": "app-visualize-window",
    "storage.local.restore": "widget.window.store",
    "app.visualize.initOptions": null,
    "name": "@ui.param.title",
    "ui.basic.elements": [
        {
            "ui.basic.tag": "div",
            "ui.basic.var": "terms",
            "ui.class.class": "app.visualize.terms",
            "core.property.group": [
                {
                    "ui.group.data": {
                        "ui.data.keyList": [
                            "app.visualize.term"
                        ],
                        "ui.data.default": {
                            "ui.basic.tag": "div",
                            "ui.class.class": "app.visualize.term"
                        },
                        "ui.data.values": [
                            "ui.param.terms"
                        ]
                    }
                }
            ],
            "lib.interact.draggable": {
                "ui.basic.tag": "div"
            }
        },
        "app.visualize.transform.popup"
    ]
}