{
    "title": "Visualize",
    "ui.property.style": {
        "left": "50px",
        "top": "50px",
        "width": "600px",
        "height": "600px"
    },
    "maximize": null,
    "icon": "/packages/res/icons/visualize.png",
    "widget.menu.items": [
        {
            "text": "Transform",
            "select": [
                {
                    "text": "Clear",
                    "select": "app.visualize.clear"
                },
                "app.visualize.transform.menu"
            ]
        },
        {
            "text": "Content",
            "select": [
                "app.visualize.content.menu"
            ]
        }
    ],
    "storage.local.key": "app-visualize-window",
    "storage.local.restore": "widget.window.store",
    "app.visualize.initOptions": null,
    "name": "@ui.param.title",
    "ui.basic.elements": [
        {
            "ui.basic.tag": "div",
            "ui.class.class": "app.visualize.terms.container",
            "ui.basic.elements": [
                {
                    "ui.basic.tag": "div",
                    "ui.basic.var": "terms",
                    "ui.class.class": "app.visualize.terms",
                    "ui.attribute.tabIndex": "0",
                    "ui.key.press": "app.visualize.keyPress",
                    "ui.key.up": "app.visualize.keyUp",
                    "ui.touch.move": "app.visualize.touchMove",
                    "core.property.group": [
                        {
                            "ui.group.data": {
                                "ui.data.keyList": [
                                    "app.visualize.term"
                                ],
                                "ui.data.default": {
                                    "ui.basic.tag": "div",
                                    "ui.class.class": "app.visualize.term",
                                    "ui.move.extend": "this",
                                    "ui.move.enabled": true,
                                    "ui.move.method": "app.visualize.moveTerm"
                                },
                                "ui.data.values": [
                                    "ui.param.terms"
                                ]
                            }
                        }
                    ]
                }
            ]
        },
        {
            "ui.class.class": "app.visualize.iconbar",
            "ui.basic.var": "iconbar",
            "ui.group.data": {
                "ui.data.keyList": [
                    "ui.basic.html",
                    "ui.attribute.title",
                    "ui.touch.click",
                    "ui.class.add",
                    "ui.basic.var"
                ],
                "ui.data.default": {
                    "ui.basic.tag": "a",
                    "href": "#"
                },
                "ui.data.values": [
                    [
                        "@ui.html.icon(fa fa-expand)",
                        "Fullscreen",
                        "widget.window.fullscreen",
                        "app-visualize-iconbar-fullscreen"
                    ],
                    [
                        "@ui.html.icon(fas fa-sort-numeric-down)",
                        "Sort by Order (Ascending)",
                        "app.visualize.sortDown",
                        "seperator"
                    ],
                    [
                        "@ui.html.icon(fas fa-sort-numeric-up)",
                        "Sort by Order (Descending)",
                        "app.visualize.sortUp"
                    ],
                    [
                        "@ui.html.icon(fas fa-sort-alpha-down)",
                        "Sort by Text (Ascending)",
                        "app.visualize.sortDown(text)"
                    ],
                    [
                        "@ui.html.icon(fas fa-sort-alpha-up)",
                        "Sort by Text (Descending)",
                        "app.visualize.sortUp(text)"
                    ],
                    [
                        "@ui.html.icon(fas fa-sort-amount-down)",
                        "Sort by Phase (Ascending)",
                        "app.visualize.sortDown(phase)"
                    ],
                    [
                        "@ui.html.icon(fas fa-sort-amount-up)",
                        "Sort by Phase (Descending)",
                        "app.visualize.sortUp(phase)"
                    ],
                    [
                        "@ui.html.icon(fas fa-tags)",
                        "Sort by Category (Ascending)",
                        "app.visualize.sortDown(phase/category)",
                        "asc"
                    ],
                    [
                        "@ui.html.icon(fas fa-tags)",
                        "Sort by Category (Descending)",
                        "app.visualize.sortUp(phase/category)",
                        "desc"
                    ]
                ]
            }
        },
        "app.visualize.transform.popup"
    ],
    "update": null
}