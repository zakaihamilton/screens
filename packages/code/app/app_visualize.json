{
    "title": "Visualize",
    "ui.property.style": {
        "left": "50px",
        "top": "50px",
        "width": "600px",
        "height": "600px"
    },
    "icon": "/packages/res/icons/diagram.png",
    "widget.menu.items": [
        {
            "text": "Transform",
            "select": [
                "app.visualize.transform.menu"
            ]
        }
    ],
    "storage.local.key": "app-visualize-window",
    "storage.local.restore": "widget.window.store",
    "app.visualize.initOptions": null,
    "name": "@ui.param.title",
    "ui.basic.elements": [
        {
            "ui.basic.tag": "div",
            "ui.basic.var": "terms",
            "ui.property.style": {
                "left": "0px",
                "top": "0px",
                "right": "0px",
                "bottom": "0px",
                "position": "absolute",
                "color": "var(--color)"
            },
            "core.property.group": [
                {
                    "ui.group.data": {
                        "ui.data.keyList": [
                            "app.visualize.term"
                        ],
                        "ui.data.default": {
                            "ui.basic.tag": "div",
                            "ui.property.style": {
                                "lineHeight": "2em",
                                "border": "1px solid black",
                                "padding": "30px",
                                "textAlign": "center",
                                "verticalAlign": "middle",
                                "boxSizing": "border-box",
                                "position": "absolute",
                                "borderRadius": "10px",
                                "boxShadow": "0 0 5px rgba(81, 203, 238, 1)"
                            }
                        },
                        "ui.data.values": [
                            "ui.param.terms"
                        ]
                    }
                }
            ],
            "lib.interact.draggable": {
                "ui.basic.tag": "div"
            }
        },
        "app.visualize.transform.popup"
    ]
}