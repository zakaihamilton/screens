{
    "title": "Visualize",
    "ui.property.style": {
        "left": "50px",
        "top": "50px",
        "width": "600px",
        "height": "600px"
    },
    "icon": "/packages/res/icons/diagram.png",
    "widget.menu.items": [
        {
            "text": "Transform",
            "select": [
                "app.visualize.transform.menu"
            ]
        }
    ],
    "storage.local.key": "app-visualize-window",
    "storage.local.restore": "widget.window.store",
    "app.visualize.initOptions": null,
    "name": "@ui.param.title",
    "ui.basic.elements": [
        {
            "ui.basic.tag": "div",
            "ui.basic.var": "terms",
            "ui.class.class": "app.visualize.terms",
            "core.property.group": [
                {
                    "ui.group.data": {
                        "ui.data.keyList": [
                            "app.visualize.term"
                        ],
                        "ui.data.default": {
                            "ui.basic.tag": "div",
                            "ui.class.class": "app.visualize.term"
                        },
                        "ui.data.values": [
                            "ui.param.terms"
                        ]
                    }
                }
            ],
            "lib.interact.draggable": {
                "ui.basic.tag": "div"
            }
        },
        {
            "ui.class.class": "app.visualize.iconbar",
            "ui.basic.var": "iconbar",
            "ui.group.data": {
                "ui.data.keyList": [
                    "ui.basic.html",
                    "ui.attribute.title",
                    "ui.touch.click",
                    "ui.class.add",
                    "ui.basic.var"
                ],
                "ui.data.default": {
                    "ui.basic.tag": "a",
                    "href": "#"
                },
                "ui.data.values": [
                    [
                        "@ui.html.icon(fa fa-expand)",
                        "Fullscreen",
                        "widget.window.fullscreen"
                    ],
                    [
                        "@ui.html.icon(fas fa-sort)",
                        "Sort by Order",
                        "app.visualize.sort",
                        "seperator"
                    ],
                    [
                        "@ui.html.icon(fas fa-sort-alpha-down)",
                        "Sort by Text",
                        "app.visualize.sort(text)"
                    ],
                    [
                        "@ui.html.icon(fas fa-sort-amount-down)",
                        "Sort by Phase",
                        "app.visualize.sort(phase)"
                    ]
                ]
            }
        },
        "app.visualize.transform.popup"
    ],
    "ui.property.after": {
        "app.visualize.sort": null,
        "timeout": 100
    },
    "update": null
}