{
    "title": "Theme",
    "ui.property.style": {
        "left": "50px",
        "top": "50px",
        "width": "500px",
        "height": "220px"
    },
    "icon": "/packages/res/icons/theme.png",
    "maximize": true,
    "widget.menu.items": [
        [
            "Theme",
            [
                "ui.theme.menuList",
                [
                    "Night Mode",
                    "ui.theme.nightMode",
                    {
                        "separator": true,
                        "state": "select"
                    }
                ]
            ]
        ]
    ],
    "ui.property.after": {
        "storage.local.key": "app-theme-window",
        "storage.local.restore": "widget.window.store"
    }
}