{
    "title": "Notes",
    "ui.property.style": {
        "left": "50px",
        "top": "50px",
        "width": "600px",
        "height": "600px"
    },
    "maximize": null,
    "icon": "notes.png",
    "widget.menu.items": [
        {
            "text": "Notes",
            "select": [
                {
                    "text": "Clear",
                    "select": "app.notes.clear"
                },
                "widget.window.exportMenuList(app.notes.exportText)"
            ]
        },
        {
            "text": "Content",
            "select": [
                "app.notes.content.menu"
            ]
        }
    ],
    "ui.basic.elements": [
        {
            "ui.basic.var": "editor",
            "ui.element.component": "widget.richeditor",
            "ui.property.after": {
                "storage.local.key": "@ui.windowkey.app-notes-editor",
                "storage.local.restore": "contents"
            }
        },
        "app.notes.transform.popup"
    ],
    "ui.property.after": {
        "storage.local.key": "app-notes-window",
        "storage.local.restore": "widget.window.store",
        "title": "Notes",
        "app.notes.initOptions": null,
        "app.notes.reload": null
    }
}