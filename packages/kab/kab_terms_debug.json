    "format": [
        {
            "find": "/\"",
            "replace": "\\\""
        },
        {
            "enabled":false,
            "find": "  ",
            "replace": " "
        },
        {
            "find": "/:",
            "replace": "\":{</br>\"expansion\":[\"",
            "prefix": "\"",
            "suffix":"\"]</br>}"
        },
        {
            "find": "/\\. ",
            "replace": "\"]</br>},</br>\""
        },
        {
            "find": "/\\.",
            "replace":""
        }
    ]
}
