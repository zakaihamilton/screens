{
    "title": "Cache",
    "ui.property.style":{
        "left": "150px",
        "top": "150px",
        "width": "450px",
        "height": "250px"
    },
    "fixed": true,
    "widget.menu.items": [
        ["Special", [
                ["Clear All", "app.cache.clearAll"]
            ]
        ]
    ],
    "ui.basic.elements": [
        {
            "ui.style.marginTop": "15px",
            "ui.basic.text": "Source:"
        },
        {
            "ui.basic.var": "storage",
            "ui.basic.text": "Local",
            "ui.style.width": "400px",
            "ui.element.count": 0,
            "core.link.onChange": "app.cache.onChangeStorage",
            "ui.group.data": {
                "ui.data.keyList": ["ui.basic.text"],
                "ui.data.default": {
                    "group": "one",
                    "state": false
                },
                "ui.data.values": [
                    ["Local"],
                    ["Session"]
                ]
            }
        },
        {
            "ui.basic.text": "Key:"
        },
        {
            "ui.basic.var": "key",
            "ui.basic.text": "",
            "ui.basic.readOnly": false,
            "ui.style.width": "400px",
            "ui.element.count": 0,
            "core.link.onChange": "app.cache.onChangeKey",
            "ui.monitor.change": "app.cache.onChangeKey",
            "ui.group.data": {
                "ui.data.keyList": ["ui.basic.text"],
                "ui.data.default": {
                    "group": "one",
                    "state": false
                },
                "ui.data.values": [
                    "app.cache.keyList"
                ]
            }
        },
        {
            "ui.basic.text": "Value:"
        },
        {
            "ui.basic.var": "value",
            "ui.basic.text": "",
            "ui.style.width": "395px",
            "ui.basic.type": "text"
        },
        {
            "ui.style.marginTop":"15px",
            "ui.basic.text": "Clear",
            "ui.touch.click": "app.cache.clear"
        },
        {
            "ui.basic.text": "Set",
            "ui.touch.click": "app.cache.store"
        }
    ],
    "ui.property.after":{
        "storage.cache.key": "app-cache-window",
        "storage.cache.restore": "widget.window.store"
    }
}