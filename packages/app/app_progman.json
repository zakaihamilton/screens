{
    "title": "Program Manager",
    "icon": "/packages/res/icons/program_manager.png",
    "ui.style.left": "750px",
    "ui.style.top": "150px",
    "ui.style.width": "400px",
    "ui.style.height": "400px",
    "widget.window.static": true,
    "widget.menu.items": [
        ["File", [
                ["New..."],
                ["Open"],
                ["Move..."],
                ["Copy..."],
                ["Delete"],
                ["Properties..."],
                ["Run...", null, {
                        "separator": true
                    }
                ],
                ["Exit Windows...", null, {
                        "separator": true
                    }
                ]
            ]],
        ["Options", [
                ["Auto Arrange", "app.progman.auto_arrange", {
                        "state": "select"
                    }
                ],
                ["Minimize on Use", "app.progman.minimize_on_use", {
                        "state": "select"
                    }
                ],
                ["Save Settings on Exit", "app.progman.save_on_exit", {
                        "state": "select"
                    }
                ]
            ]],
        ["Window", [
                ["Cascade", "ui.arrange.cascade"],
                ["Tile Horizontally", "ui.arrange.tileHorizontally"],
                ["Tile Vertically", "ui.arrange.tileVertically"],
                ["Arrange Icons", "ui.arrange.arrangeIcons"],
                "widget.window.childMenuList"
            ]],
        ["Help", [
                ["Contents"],
                ["Search for Help on..."],
                ["How to Use Help"],
                ["Windows Tutorial"],
                ["About Program Manager..."]
            ]]
    ],
    "ui.group.data": {
        "ui.data.default": {
            "icon": "/packages/res/icons/group.png",
            "minimize": null,
            "static": true,
            "ui.property.after":{
                "storage.cache.key": "@ui.cachekey.app-progman-window-",
                "storage.cache.restore": "widget.window.store"
            }
        },
        "ui.data.keyList": [
            "title",
            "ui.basic.elements"
        ],
        "ui.data.values": [
            [
                "Main", [
                    {
                        "text": "Transform",
                        "ui.basic.src": "/packages/res/icons/transform.png",
                        "app.progman.args": "transform",
                        "ui.touch.dblclick": "app.progman.shell"
                    },
                    {
                        "text": "Player",
                        "ui.basic.src": "/packages/res/icons/default.png",
                        "app.progman.args": "player",
                        "ui.touch.dblclick": "app.progman.shell"
                    }
                ]
            ],
            [
                "Accessories", [
                    {
                        "text": "Browser",
                        "ui.basic.src": "/packages/res/icons/default.png",
                        "app.progman.args": "browser",
                        "ui.touch.dblclick": "app.progman.shell"
                    },
                    {
                        "text": "Theme",
                        "ui.basic.src": "/packages/res/icons/default.png",
                        "app.progman.args": "theme",
                        "ui.touch.dblclick": "app.progman.shell"
                    }
                ]
            ],
            [
                "Utilities", [
                    {
                        "text": "Terminal",
                        "ui.basic.src": "/packages/res/icons/terminal.png",
                        "app.progman.args": "terminal",
                        "ui.touch.dblclick": "app.progman.shell"
                    },
                    {
                        "text": "Controls",
                        "ui.basic.src": "/packages/res/icons/controls.png",
                        "app.progman.args": "controls",
                        "ui.touch.dblclick": "app.progman.shell"
                    },
                    {
                        "text": "Canvas",
                        "ui.basic.src": "/packages/res/icons/default.png",
                        "app.progman.args": "canvas",
                        "ui.touch.dblclick": "app.progman.shell"
                    },
                    {
                        "text": "Cache",
                        "ui.basic.src": "/packages/res/icons/default.png",
                        "app.progman.args": "cache",
                        "ui.touch.dblclick": "app.progman.shell"
                    },
                    {
                        "text": "Storage",
                        "ui.basic.src": "/packages/res/icons/default.png",
                        "app.progman.args": "storage",
                        "ui.touch.dblclick": "app.progman.shell"
                    },
                    {
                        "text": "Execute",
                        "ui.basic.src": "/packages/res/icons/default.png",
                        "app.progman.args": "execute",
                        "ui.touch.dblclick": "app.progman.shell"
                    },
                    {
                        "text": "Folder",
                        "ui.basic.src": "/packages/res/icons/folder.svg",
                        "app.progman.args": "folder .",
                        "ui.touch.dblclick": "app.progman.shell"
                    }
                ]
            ]
        ]
    },
    "ui.property.after":{
        "storage.cache.key": "app-progman-window",
        "storage.cache.restore": "widget.window.store"
    }
}
