{
    "title": "Table of Phases",
    "layers": [
        {
            "ui.property.style": {
                "display": "grid",
                "position": "absolute",
                "margin": "2px",
                "left": "0px",
                "top": "0px",
                "right": "0px",
                "bottom": "0px",
                "gridAutoColumns": "auto",
                "gridAutoRows": "auto",
                "gridTemplateRows": "3em"
            },
            "core.property.group": [
                {
                    "ui.group.data": {
                        "ui.data.keyList": [
                            "ui.style.gridRow",
                            "ui.style.gridColumn",
                            "app.diagram.term",
                            "ui.class.add"
                        ],
                        "ui.data.default": {
                            "ui.basic.tag": "div",
                            "ui.property.style": {
                                "lineHeight": "2em",
                                "border": "1px solid black",
                                "margin": "3px",
                                "padding": "3px",
                                "gridColumn": "1",
                                "textAlign": "center",
                                "verticalAlign": "middle"
                            }
                        },
                        "ui.data.values": [
                            "ui.param.gridData"
                        ]
                    }
                }
            ]
        }
    ]
}